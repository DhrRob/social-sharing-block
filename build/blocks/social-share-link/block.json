{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "outermost/social-share-link",
	"title": "Social Share Link",
    "parent": [ "outermost/social-share" ],
	"description": "Display an icon that shares the current page when clicked.",
	"textdomain": "the-social-share-block",
    "attributes": {
        "service": {
            "type": "string"
        },
        "label": {
            "type": "string"
        }
    },
    "usesContext": [
		"showLabels",
		"iconColorValue",
		"iconBackgroundColorValue"
	],
    "supports": {
		"reusable": false,
		"html": false
	},
	"editorStyle": "file:./../../editor.css"
}
