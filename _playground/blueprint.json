{
    "$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"features": {
		"networking": true
	},
	"steps": [
        {
			"step": "login"
		},
		{
			"step": "installPlugin",
			"pluginZipFile": {
				"resource": "wordpress.org/plugins",
				"slug": "social-sharing-block"
			},
			"options": {
				"activate": true
			}
		},
        {
            "step": "importFile",
            "file": {
                "resource": "url",
                "url": "https://raw.githubusercontent.com/ndiego/social-sharing-block/main/_playground/export.xml"
            }
        }
	],
    "landingPage": "/wp-admin/post.php?post=5&action=edit"
}